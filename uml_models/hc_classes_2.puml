@startuml
title   HealthCalculator5



skinparam class {
BackgroundColor White
ArrowColor Black
BorderColor Black
}
skinparam object {
BackgroundColor White
ArrowColor Black
BorderColor Black
}
scale 1.3
skinparam DefaultTextAlignment left
skinparam shadowing false
skinparam packageStyle frame
hide circle
'hide members
'skinparam monochrome true
'note " lots in deal" as spread
'package Storage {
class Heart{
gender
pulse
}
class Resp{
gender
time
}
class IMT{
weight
height
}
class Health{
sum_h_level
add_subsys()
diagram()
}
class Subsys{
data
load()
calibrate()
}
class User{
pages
input()
output()
}
class Harrington1{
x_begin
x_end
}
class Harrington2{
x_optimum
}
class Harrington{
h_level
load()
calc()
}
Harrington2 "1"-"2" Harrington1
Heart -|> Resp
IMT --|> Subsys
Resp --|> Subsys
User -- Health
'Subsys <|-  Resp
Health "1"-"*"  Subsys
Harrington2 --|> Harrington
Harrington1 --|> Harrington
Subsys - Harrington
@enduml




